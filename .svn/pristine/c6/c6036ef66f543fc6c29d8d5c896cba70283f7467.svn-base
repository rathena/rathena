		// Emote[58-63]:
		// e_dice1, e_dice2, e_dice3, e_dice4, e_dice5, e_dice6
		emotion 58+.@pc_dic,1;
		emotion 58+.@npc_dic;

		if (.@pc_dic > .@npc_dic) {
			mes "[KungKung]";
